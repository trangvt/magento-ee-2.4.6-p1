<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="StorefrontPurchaseOrderRuleDeleteModalSection">
        <element name="modal" type="text" selector="//aside[contains(@class, 'modal-popup confirm') and contains(@class,'_show')]"/>
        <element name="overlay" type="text" selector="//div[@class='modals-overlay']"/>
        <element name="modalTitle" type="text" parameterized="true" selector="//aside[contains(@class, 'modal-popup confirm') and contains(@class,'_show')]//h1[contains(text(),'{{title}}')]"/>
        <element name="modalText" type="text" selector="//aside[contains(@class, 'modal-popup confirm') and contains(@class,'_show')]//div[@data-role='content']/div"/>
        <element name="modalClose" type="button" selector="//aside[contains(@class, 'modal-popup confirm') and contains(@class,'_show')]//button[@data-role='closeBtn']"/>
        <element name="cancel" type="button" selector="//aside[contains(@class, 'modal-popup confirm') and contains(@class,'_show')]//button[contains(@class, 'action-dismiss')]"/>
        <element name="ok" type="button" selector="//aside[contains(@class, 'modal-popup confirm') and contains(@class,'_show')]//button[contains(@class, 'action-accept')]"/>
    </section>
</sections>
