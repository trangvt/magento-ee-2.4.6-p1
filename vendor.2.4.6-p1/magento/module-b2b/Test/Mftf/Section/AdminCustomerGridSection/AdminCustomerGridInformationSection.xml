<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminCustomerGridInformationSection" deprecated="Use AdminCustomerGridSection">
        <element name="status" type="text" selector="//tr[@class='data-row' and //div[text()='{{customerEmail}}']]//div[contains(text(), '{{status}}')]" parameterized="true" timeout="30" deprecated="Use {{AdminCustomerGridSection.status}}"/>
        <element name="associatedCompany" type="text" selector="//tr[@class='data-row' and //div[text()='{{customerEmail}}']]//div[contains(text(), '{{companyName}}')]" parameterized="true" timeout="30" deprecated="Use {{AdminCustomerGridSection.associatedCompany}}"/>
    </section>
</sections>
