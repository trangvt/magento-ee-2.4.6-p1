        <data key="streetAddress">9922 Street Name</data>
        <data key="city">Mukachevo</data>
        <data key="country">Ukraine</data>
        <data key="zip">89601</data>
        <data key="phone">512-345-6789</data>
        <data key="companyAdminEmail" unique="prefix">_email@admin.com</data>
        <data key="firstName" unique="suffix">John_</data>
        <data key="lastName" unique="suffix">Doe_</data>
    </entity>
</entities>
