<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="CompanyData" type="backend">
        <data key="companyName">TestCompany</data>
        <data key="companyEmail">c@c.com</data>
        <data key="streetAddress">streetAddress</data>
        <data key="city">Yerevan</data>
        <data key="zip">7777</data>
        <data key="phone">9999</data>
        <data key="companyAdminEmail" unique="prefix">email@email.com</data>
