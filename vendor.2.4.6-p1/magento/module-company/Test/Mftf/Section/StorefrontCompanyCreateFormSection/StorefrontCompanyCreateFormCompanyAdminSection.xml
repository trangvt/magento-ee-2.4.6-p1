<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="StorefrontCompanyCreateFormCompanyAdminSection">
        <element name="adminEmail" type="input" selector="#email_address" timeout="30"/>
        <element name="adminFirstName" type="input" selector="#firstname" timeout="30"/>
        <element name="adminLastName" type="input" selector="#lastname" timeout="30"/>
        <element name="adminEmailError" type="input" selector="//input[contains(@name, 'customer[email]')]/parent::div//div[contains(@class, 'mage-error')]" timeout="30"/>
    </section>
</sections>
