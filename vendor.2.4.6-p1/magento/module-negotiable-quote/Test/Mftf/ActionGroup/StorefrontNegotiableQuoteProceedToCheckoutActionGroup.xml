<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="StorefrontNegotiableQuoteProceedToCheckoutActionGroup">
        <annotations>
            <description>You are on the Storefront Negotiable Quote view page, press 'Proceed to Checkout' button</description>
        </annotations>
        <arguments>
            <argument name="proceedToCheckout" defaultValue="{{StorefrontNegotiableQuoteViewSection.proceedToCheckout}}" type="string"/>
        </arguments>
        <click selector="{{proceedToCheckout}}" stepKey="clickProceedToCheckoutButton"/>
    </actionGroup>
</actionGroups>
