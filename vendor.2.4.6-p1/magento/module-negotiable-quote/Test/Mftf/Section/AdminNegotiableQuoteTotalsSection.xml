<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminNegotiableQuoteTotalsSection">
        <element name="quoteSubtotal" type="text" selector=".quote-totals tr.proposed_quote_price span.price"/>
        <element name="quoteGrandTotal" type="text" selector=".quote-totals tr.grand_total span.price"/>
        <element name="quoteCatalogTotal" type="text" selector=".quote-totals tr.catalog_price_table table.toggle-action tr.catalog_price span.price"/>
        <element name="quoteCatalogSubtotal" type="text" selector=".quote-totals tr.catalog_price_table div#toggle-part tr.subtotal span.price"/>
        <element name="quoteCatalogSubtotalInclTax" type="text" selector=".quote-totals tr.catalog_price_table div#toggle-part tr.subtotalTax span.price"/>
        <element name="catalogTax" type="text" selector=".quote-totals tr.catalog_price_table div#toggle-part tr.tax span.price"/>
        <element name="totalTax" type="text" selector=".quote-totals tr.quote_tax span.price"/>
    </section>
</sections>
