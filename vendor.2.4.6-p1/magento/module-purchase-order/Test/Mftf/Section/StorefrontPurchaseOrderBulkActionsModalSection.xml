<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="StorefrontPurchaseOrderBulkActionsModalSection">
        <element name="popupConfirm" type="button" selector="aside.confirm" timeout="30"/>
        <element name="modalContent" type="button" selector="aside.confirm .modal-content > div" timeout="30"/>
        <element name="ok" type="button" selector="aside.confirm .modal-footer .action-primary" timeout="30"/>
        <element name="cancel" type="button" selector="aside.confirm .modal-footer .action-secondary" timeout="30"/>
    </section>
</sections>
